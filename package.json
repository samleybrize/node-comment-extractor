    "keywords": [
        "comment-parser",
        "comment",
        "parser",
        "todo"
    ],
    "version": "0.8.0",
    "repository": "samleybrize/node-comment-extractor",
    "author": "Stephen Berquet",
    "license": "MIT",
    "engines": {
        "node": ">= 0.10"
    },
    "main": "src/index.js",
    "scripts": {
        "postinstall": "typings install",
        "typings": "typings",
